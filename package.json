{
  "name": "@ankurah/react-forms",
  "version": "0.1.0",
  "description": "DRY form components for Ankurah entities with overlay-based staged edits",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ankurah/react-forms.git"
  },
  "bugs": {
    "url": "https://github.com/ankurah/react-forms/issues"
  },
  "homepage": "https://github.com/ankurah/react-forms#readme",
  "scripts": {
    "build": "tsup src/index.tsx --format esm --dts --external react",
    "dev": "tsup src/index.tsx --format esm --dts --external react --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "ankurah",
    "react",
    "forms",
    "crdt"
  ],
  "license": "MIT"
}
